Cell[BoxData[
    RowBox[{
      RowBox[{\(BarCar[f_, cA_, cB_, cC_]\), ":=", 
        RowBox[{"f", " ", "/.", "\[IndentingNewLine]", 
          RowBox[{"{", 
            RowBox[{
              RowBox[{"x", "\[Rule]", " ", 
                RowBox[{"Det", "[", 
                  RowBox[{"(", GridBox[{
                        {\(cB\[LeftDoubleBracket]1\[RightDoubleBracket] - 
